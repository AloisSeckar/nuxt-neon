{
  "name": "nuxt-neon",
  "version": "0.7.1",
  "description": "Nuxt framework integration with Neon database",
  "repository": "github:AloisSeckar/nuxt-neon",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "pnpm dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "pnpm lint && pnpm test && pnpm prepack && changelogen --release && pnpm publish && git push --follow-tags",
    "lint": "eslint .",
    "test:unit": "vitest run test/test-unit",
    "test:e2e": "vitest run test/test-e2e",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@neondatabase/serverless": "1.0.2",
    "@nuxt/kit": "4.2.2",
    "sqlstring": "2.3.3",
    "vite-plugin-commonjs": "0.10.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "3.1.1",
    "@nuxt/eslint-config": "1.12.1",
    "@nuxt/module-builder": "1.0.2",
    "@nuxt/schema": "4.2.2",
    "@types/node": "25.0.2",
    "changelogen": "0.6.2",
    "eslint": "9.39.2",
    "nuxt": "4.2.2",
    "nuxt-spec": "0.1.16",
    "typescript": "5.9.3",
    "vue-tsc": "3.1.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.25.0"
}
